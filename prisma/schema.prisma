generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model healthkit_record {
  id        Int      @id @default(autoincrement())
  synced_at DateTime @default(now())
  hash      String   @unique
  date      DateTime
  type      String
  value     Float
  unit      String
}
